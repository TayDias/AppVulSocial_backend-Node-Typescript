{
  "name": "backend-proj-vs",
  "version": "1.0.0",
  "description": "Social vulnerability Api",
  "main": "dist/src/server.js",
  "types": "dist/src/server.d.ts",
  "declaration": true,
  "scripts": {
    "dev": "npx ts-node-dev --ignore-watch node_modules --respawn src/server.ts",

    "knex:migrate": "npx knex --knexfile knexfile.ts migrate:latest --env development",
    "knex:seed": "npx knex --knexfile knexfile.ts seed:run --env development",
    "knex:migrate2": "npx knex --knexfile knexfile.ts migrate:latest --env development2",
    "knex:seed2": "npx knex --knexfile knexfile.ts seed:run --env development2",
    
    "knex:migrateprod": "npx knex --knexfile knexfile.ts migrate:latest --env production",
    "knex:seedprod": "npx knex --knexfile knexfile.ts seed:run --env production",

    "start": "node dist/src/server.js",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Taynara",
  "license": "Personal",
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "cors": "^2.8.5",
    "crypto-ts": "^1.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.5",
    "mysql": "^2.18.1",
    "node-pre-gyp": "^0.15.0"
  },
  "devDependencies": {
    "sqlite3": "^5.0.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  }
}
